        <Button HorizontalAlignment="Right" VerticalAlignment="Top" Width="50" Height="50" Click="Button_Click" Background="{x:Null}" BorderBrush="{x:Null}">

            <Image Source="/Images/загруженное (1).png"/>

        </Button>
    </Grid>
